		"imports-loader": "^1.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"sass-loader": "^9.0.2",
		"val-loader": "^2.1.1",
		"web-ext": "^4.3.0",
		"web-ext-types": "^3.2.1",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
